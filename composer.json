{
    "name": "reyworkbench/msjframework",
    "description": "MSJ Framework - Base Framework for Laravel MSJ Framework",
    "keywords": [
        "laravel",
        "crud",
        "generator",
        "scaffolding",
        "msj",
        "framework",
        "laravel-prompts",
        "artisan",
        "code-generator",
        "blade",
        "model-generator",
        "controller-generator"
    ],
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Reynald Silva",
            "email": "reynaldsilva123@gmail.com",
            "homepage": "https://github.com/rey-workbench/msjframework",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/rey-workbench/msjframework",
    "support": {
        "issues": "https://github.com/rey-workbench/msjframework/issues",
        "source": "https://github.com/rey-workbench/msjframework",
        "docs": "https://github.com/rey-workbench/msjframework#readme"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/rey-workbench/msjframework.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "laravel/prompts": "^0.1.0|^0.3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "phpunit/phpunit": "^11.0",
        "barryvdh/laravel-ide-helper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MSJFramework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MSJFramework\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MSJFramework\\ServiceProvider"
            ],
            "aliases": {}
        }
    },
    "scripts": {
        "test": "phpunit",
        "pint": "pint",
        "pint:test": "pint --test",
        "build": "php build/build.php --clean --verbose",
        "build:dev": "php build/build.php --clean --no-obfuscate --verbose",
        "build:prod": "php build/build.php --clean --verbose"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
